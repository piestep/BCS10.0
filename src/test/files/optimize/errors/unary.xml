<?xml version="1.0"?>
<body>

<test>
<name>Unary_Boolean.</name>
<code>
<ln>package body TEST is</ln>
<ln></ln>
<ln>   type FALSE_TYPE is new BOOLEAN range FALSE .. FALSE;</ln>
<ln>   A : FALSE_TYPE := FALSE;</ln>
<ln>   B : FALSE_TYPE;</ln>
<ln></ln>
<ln>   procedure MAIN is</ln>
<ln>   begin</ln>
<ln>      B := not (A AND FALSE);</ln>
<ln>   end MAIN;</ln>
<ln>end TEST;</ln>
</code>
<listing>
<ln>9, 17: Expression not within type (2).</ln>
</listing>
</test>

<test>
<name>Unary_Integer.</name>
<code>
<ln>package body TEST is</ln>
<ln></ln>
<ln>   type INDEX_TYPE is new INTEGER range 0 .. 10;</ln>
<ln>   A : INDEX_TYPE := 10;</ln>
<ln>   B : INDEX_TYPE;</ln>
<ln></ln>
<ln>   procedure MAIN is</ln>
<ln>   begin</ln>
<ln>      B := - (2 - 1);</ln>
<ln>   end MAIN;</ln>
<ln>end TEST;</ln>
</code>
<listing>
<ln>9, 17: Expression not within type (2).</ln>
</listing>
</test>
</body>
