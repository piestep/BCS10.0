<?xml version="1.0"?>
<body>

<test>
<name>Parameter_Identifier.</name>
<code>
<ln>package body TEST is</ln>
<ln></ln>
<ln>   procedure MAIN (MAIN : in INTEGER) is</ln>
<ln>   begin</ln>
<ln>      null;</ln>
<ln>   end MAIN;</ln>
<ln>end TEST;</ln>
</code>
<listing>
<ln>3, 24: Identifier already defined (3).</ln>
</listing>
</test>

<test>
<name>Parameter_Excessive.</name>
<code>
<ln>package body TEST is</ln>
<ln></ln>
<ln>   procedure MAIN (A : in INTEGER) is</ln>
<ln>   begin</ln>
<ln>      null;</ln>
<ln>   end MAIN;</ln>
<ln>end TEST;</ln>
</code>
<listing>
<ln>3, 34: Warning: Integer type may cause excessive BCode compilation (1).</ln>
</listing>
</test>

<test>
<name>Parameter_Expected.</name>
<code>
<ln>package body TEST is</ln>
<ln></ln>
<ln>   procedure MAIN (A : in TEST) is</ln>
<ln>   begin</ln>
<ln>      null;</ln>
<ln>   end MAIN;</ln>
<ln>end TEST;</ln>
</code>
<listing>
<ln>3, 31: Expected type identifier (1).</ln>
</listing>
</test>

<test>
<name>Parameter_Undefined.</name>
<code>
<ln>package body TEST is</ln>
<ln></ln>
<ln>   procedure MAIN (A : in INDEX_TYPE) is</ln>
<ln>   begin</ln>
<ln>      null;</ln>
<ln>   end MAIN;</ln>
<ln>end TEST;</ln>
</code>
<listing>
<ln>3, 37: Undefined type identifier (1).</ln>
</listing>
</test>
</body>

